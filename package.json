{
  "name": "zce-cli",
  "version": "0.3.0-alpha.10",
  "description": "A CLI tool for my personal productivity.",
  "keywords": [
    "productivity",
    "scaffolding",
    "generator",
    "cli",
    "typescript",
    "jest",
    "node",
    "zce"
  ],
  "homepage": "https://github.com/zce/zce-cli#readme",
  "bugs": {
    "url": "https://github.com/zce/zce-cli/issues",
    "email": "w@zce.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/zce-cli.git"
  },
  "license": "MIT",
  "author": {
    "name": "zce",
    "email": "w@zce.me",
    "url": "https://zce.me"
  },
  "main": "lib/index.js",
  "bin": {
    "zce": "bin/zce.js"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "doc": "docs",
    "test": "test"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src",
    "compile": "tsc",
    "build": "run-s clean lint compile",
    "test:unit": "jest src",
    "test:integration": "jest test",
    "test": "run-p test:*",
    "cov": "jest --coverage",
    "prepare": "run-s build test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "standard-ts",
    "parserOptions": {
      "project": "tsconfig.eslint.json"
    },
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "tag": "next"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "enquirer": "^2.3.6",
    "execa": "^4.0.3",
    "extract-zip": "^2.0.1",
    "glob": "^7.1.6",
    "got": "^11.5.0",
    "ini": "^1.3.5",
    "lodash": "^4.17.19",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5",
    "minimist-options": "^4.1.0",
    "ora": "^4.0.4",
    "redent": "^3.0.0",
    "semver": "^7.3.2",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@types/ini": "^1.3.30",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.157",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.0.22",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.1",
    "@types/validate-npm-package-name": "^3.0.0",
    "eslint": "^7.4.0",
    "eslint-config-standard-ts": "^18.0.2",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">=12.10"
  }
}
